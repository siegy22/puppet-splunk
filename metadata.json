{
  "name": "puppet-splunk",
  "version": "9.1.2-rc0",
  "author": "Vox Pupuli",
  "summary": "Manage and deploy Splunk servers and forwarders",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-splunk",
  "project_page": "https://github.com/voxpupuli/puppet-splunk",
  "issues_url": "https://github.com/voxpupuli/puppet-splunk/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Rocky",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 8.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.25.0 < 9.0.0"
    },
    {
      "name": "puppetlabs-inifile",
      "version_requirement": ">= 1.4.1 < 6.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 4.2.1 < 8.0.0"
    },
    {
      "name": "puppet-archive",
      "version_requirement": ">= 2.0.0 < 7.0.0"
    }
  ]
}
